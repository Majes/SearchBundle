<div class="col-lg-9">
    <div class="widget-container fluid-height clearfix">
        <div class="heading">
            <i class="icon-search"></i> {% trans %}Results{% endtrans %}
        </div>

        <div>
            {% for entity in results %}
            {% set fields = entity.entityRender() %}
                <div class="row">
                    <div class="col-md-4">{{fields.title}}</div>
                    <div class="col-md-4">{{fields.description}}</div>
                    <div class="col-md-4">{{fields.url}}</div>
                </div>
            {% endfor %}    
        </div>

    </div>

</div>